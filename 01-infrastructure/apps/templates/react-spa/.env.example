# React SPA Environment Variables Template
# Copy to .env and fill in your values

# Supabase Configuration (injected at build time)
VITE_SUPABASE_URL=https://api.yourdomain.com
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# For Create React App, use REACT_APP_ prefix instead:
# REACT_APP_SUPABASE_URL=https://api.yourdomain.com
# REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# API Endpoints
VITE_API_URL=https://api.yourdomain.com

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# App Configuration
VITE_APP_NAME=My App
VITE_APP_VERSION=1.0.0
